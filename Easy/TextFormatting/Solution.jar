import java.util.*;
import java.io.*;
import java.math.*;

/**
 * Auto-generated code below aims at helping you parse
 * the standard input according to the problem statement.
 **/
class Solution {
	static String punctutations = ".,:;?!";
    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        String intext = in.nextLine();
        
        intext = deleteUnnecessarieSign(intext);
        
        intext = CorrectWhitespaces(intext);
        intext = UpperCaseLetters(intext);
        intext = deleteUnnecessarieSign(intext);
        System.out.println(intext);
    }

	private static String UpperCaseLetters(String intext) {
		
		String text [] = intext.split("");
		String result ="";
		
		result=text[0].toUpperCase()+text[1].toLowerCase();
		for (int i = 2 ; i <text.length ; i++ ) {
			
			if(text[i-2].equals(".")) {
				text[i]=text[i].toUpperCase();
			}
			else {
				text[i]=text[i].toLowerCase();
			}
			result+=text[i];
		}
	
		return result;
	}



	private static String CorrectWhitespaces(String intext) {
		String text [] = intext.split("");
		String result ="";
		for(int i =2; i< text.length ; i++ ) {
			
			if(i==text.length-1 && punctutations.contains(text[i]) && text[i-1].equals(" ")) {
				text[i-1]="";
			}
		else if(punctutations.contains(text[i-1])) {
				if(text[i-2].equals(" "))
				{text[i-2]="";}
				
				if(!text[i].equals(" ")) {
					text[i]=" "+text[i];
				}
			}
			result+=text[i-2];
		}
		result+=text[text.length-2]+text[text.length-1];

		return result;
	}



	private static String deleteUnnecessarieSign(String intext) {
		
		String text [] = intext.split("");
		
		String result ="";
		for(int i =1; i<text.length;i++) {
			
			if(text[i].equals(" ")  && text[i-1].equals(" ")) {
				text[i-1]="";
			}
			else if (punctutations.contains(text[i]) && punctutations.contains(text[i-1]) ) {
				text[i-1]="";
			}
			result +=text[i-1];
		}
		result+=text[text.length-1];
		
		return result;
	}
}