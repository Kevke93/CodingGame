    public static boolean river (int r) {
    	
    	//String digits=Integer.toString(r);
    	//int amountDigits=digits.length();
    	
    	
    	for( int i=r-1; i>0;i--) {
    		int sum=i;
    		int num=i;

    		while (num>0) {
    			sum=sum+num%10;
    			num=num/10;
    		}
    		
    		if(sum==r) {return true;}
    	}
    	return false;
    }

